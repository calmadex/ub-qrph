{
    "name": "ezi/ub-qrph",
    "description": "UB QRPH - Integration",
    "license": "MIT",
    "require": {
        "illuminate/support": "*",
        "php": "^8.0|^8.1|^8.2",
        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.3|^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.18"
    },
    "authors": [
        {
            "name": "Dex",
            "email": "jdexcalma@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ezi\\UbQrPh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QrPh\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Ezi\\UbQrPh\\ServiceProvider"
            ]
        }
    }
}
